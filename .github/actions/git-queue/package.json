{
  "name": "git-queue",
  "description": "GitHub Action to build a job queue using git empty commits",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc && ncc build",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "jest --coverage",
    "pre-checkin": "yarn run format && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josecelano/library-consumer.git"
  },
  "keywords": [
    "actions",
    "concurrency",
    "git",
    "lock"
  ],
  "author": "Jose Celano",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.0",
    "simple-git": "^3.0.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@vercel/ncc": "^0.24.1",
    "jest": "^26.0.1",
    "jest-circus": "^26.0.1",
    "jest-fixtures": "^0.6.0",
    "jest-runtime": "^26.0.1",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3",
    "typescript-formatter": "^7.2.2"
  }
}
